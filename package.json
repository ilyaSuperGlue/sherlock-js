{
  "name": "@ilyasuperglue/sherlock-js",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "a library for running sherlock on JS env",
  "version": "0.0.1",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "private": false,
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "types": "bunx tsc",
    "build:all": "bun run build && bun run types"
  },
  "type": "module",
  "keywords": [
    "sherlock",
    "javascript",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ilyaSuperGlue/sherlock-js.git"
  },
  "author": "ilyas <ilyasabdurahmanyusuf@gmail.com> (https://github.com/hikaaam)",
  "license": "MIT",
  "homepage": "https://github.com/ilyaSuperGlue/sherlock-js#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "packageManager": "bun@1.3.2"
}
