{
  "name": "@ilyasuperglue/sherlock-js",
  "version": "0.0.7",
  "description": "a library for running sherlock on JS env",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "dist/index.js",
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node --format esm",
    "build:bundle": "bun run build && bunx tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {},
  "keywords": [
    "sherlock",
    "javascript",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ilyaSuperGlue/sherlock-js.git"
  },
  "author": "ilyas <ilyasabdurahmanyusuf@gmail.com> (https://github.com/ilyaSuperGlue)",
  "license": "MIT",
  "homepage": "https://github.com/ilyaSuperGlue/sherlock-js#readme",
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "packageManager": "bun@1.3.2",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ]
}
